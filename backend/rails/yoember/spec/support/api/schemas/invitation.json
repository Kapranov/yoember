{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "data": {
    "type": "object",
    "required": ["invitation"],
    "properties": {
      "invitation": {
        "type" : "object",
        "required" : [
          "id",
          "email"
        ],
        "properties" : {
          "id" : { "type" : "integer" },
          "email" : { "type" : "string" },
          "created_at" : { "type" : "string", "format": "date-time" },
          "updated_at" : { "type" : "string", "format": "date-time" }
        }
      }
    }
  }
}
